---
import { Code } from "astro:components";

type Props = {
    example?: string[];
};

const { example } = Astro.props;

const parsedExample = example?.join("").replace(/^```js\s+|```$/g, "");
---

{
    parsedExample && (
        <Code code={parsedExample} lang="js" class="p-2 rounded-md" />
    )
}
