---
import ReferenceEntry, {
    type ReferenceEntryProps,
} from "./ReferenceEntry.astro";

type Props = ReferenceEntryProps;

const { name, type } = Astro.props;
const typeMembersKey = Object.keys(type.members);
---

<div class="text-2xl flex flex-col gap-4">
    <h1 class="font-medium">{name}</h1>
    <div class="flex flex-col gap-2">
        {
            typeMembersKey.map((key: string) => (
                <div>
                    <ReferenceEntry
                        name={key}
                        type={type.members[key]}
                        level={1}
                        parent={`${name}-`}
                    />
                </div>
            ))
        }
    </div>
</div>
